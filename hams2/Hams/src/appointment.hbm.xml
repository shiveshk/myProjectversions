<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.hams.data.Appointment" table="appointment">  
    <id name="appointment_id" type="long">  
     <generator class="increment"></generator>  
    </id>  
    <property name="patient_name"  column="patient_name" />  
    <property name="clinic_detail"  column="clinic_detail" />
    <property name="time"  column="time" /> 
    
    <property name="time_stamp" column="time_stamp" /> 
    
    

 </class>  
 
 </hibernate-mapping>
 
